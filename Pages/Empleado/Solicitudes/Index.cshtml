@page
@model IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Solicitud.FirstOrDefault().Contrato.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Solicitud.FirstOrDefault().FechaSolicitud)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Solicitud.FirstOrDefault().FechaInicio)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Solicitud.FirstOrDefault().FechaFin)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Solicitud.FirstOrDefault().Motivo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Solicitud.FirstOrDefault().Descripcion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Solicitud.FirstOrDefault().Estado)
        </th>
        <th></th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model.Solicitud)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Contrato.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaSolicitud)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaInicio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaFin)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Motivo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Descripcion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Estado)
            </td>
            <td>
                <a asp-page="./Details" asp-route-id="@item.Id">Details</a>
            </td>
        </tr>
    }
    </tbody>
</table>